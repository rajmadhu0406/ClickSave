{
    "manifest_version": 3,
    "version": "1.1",
    "name": "ClickSave",
    "key": "",
    "action": {
        "default_popup": "index.html",
        "default_icon": "link.png"
    },
    "host_permissions": [
        "https://api.clickup.com/api/v2/*"
    ],
    "permissions": [
        "tabs",
        "webRequest" ,
        "storage",
        "activeTab",
        "webNavigation"
       ]
}
